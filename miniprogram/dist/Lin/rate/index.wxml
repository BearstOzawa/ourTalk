<view class="l-rate l-class">
    <view bindtap="handleClick" class="l-rate-star" data-index="{{index}}" wx:for="{{count}}" wx:key="{{item}}">
        <view class="icon-checked" data-rate="{{score-index}}" hoverClass="none" hoverStopPropagation="false" style="width:{{score-index<1?(score-index)*100:100}}%" wx:if="{{score>index}}">
            <image class="image-item l-class-image" lazyLoad="false" mode="aspectFit|aspectFill|widthFix" src="{{activeImage}}" wx:if="{{activeImage&&inActiveImage}}"></image>
            <l-icon color="{{activeColor}}" lClass="l-class-icon" name="{{name}}" size="{{size}}" wx:else></l-icon>
        </view>
        <image class="image-item l-class-image" lazyLoad="false" mode="aspectFit|aspectFill|widthFix" src="{{inActiveImage}}" wx:if="{{activeImage&&inActiveImage}}"></image>
        <l-icon color="{{inActiveColor}}" lClass="l-class-icon" name="{{name}}" size="{{size}}" wx:else></l-icon>
    </view>
</view>
